{
    "defaultSeverity": "error",
    "extends": ["tslint:recommended", "tslint-config-airbnb"],
    "jsRules": {},
    "rules": {
        "variable-name": false,
        "ordered-imports": false,
        "indent": [true, "spaces", 4],
        "ter-indent": false,
        "jsdoc-format": false,
        "max-line-length": false,
        "object-literal-sort-keys": false
